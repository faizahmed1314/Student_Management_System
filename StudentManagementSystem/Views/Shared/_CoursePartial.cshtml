@model IEnumerable<StudentManagementSystem.Models.Course>
           
<div style="font-family: Arial">
    <table id="courseTable" class="table table-bordered table-striped table-responsive" style="background-color: beige">
        <thead>
            <tr>
                <th>Course</th>
                <th>Month</th>
            </tr>
            @foreach (var item in Model)
            {
                <tr>
                    <td>@Html.DisplayFor(m=>item.course1)</td>
                    <td>@Html.DisplayFor(m => item.duration)</td>
                </tr>
            }
           
        </thead>
    </table>
</div>
